CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/HTBaseControlKit" "${PODS_CONFIGURATION_BUILD_DIR}/HTCategoryKit" "${PODS_CONFIGURATION_BUILD_DIR}/HTUIKit"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/HTBaseControlKit/HTBaseControlKit.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/HTCategoryKit/HTCategoryKit.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/HTUIKit/HTUIKit.framework/Headers"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_LDFLAGS = $(inherited) -framework "HTBaseControlKit" -framework "HTCategoryKit" -framework "HTUIKit"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
